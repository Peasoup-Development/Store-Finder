<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceBlock name="top.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="storefinder.link" before="help-link-new">
                <arguments>
                    <argument name="label" xsi:type="string" translate="false">Store Locator</argument>
                    <argument name="path" xsi:type="string" translate="false">storefinder</argument>
                </arguments>
            </block>
        </referenceBlock>

        <referenceBlock name="pslogin.customer.form.login.buttons" remove="true" />
    </body>
</page>